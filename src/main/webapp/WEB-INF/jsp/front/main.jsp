<script>
$('.business_slider').on('init',function(event, slick){
	var temp = '<div class="slick-count"><span id="slick-current1" class="font-weight-bold"></span>  /  <span id="slick-total1"></span></div>';
	$(this).append($(temp));
	addSlickNumber(slick);
})
$('.business_slider').slick({
	infinite: false,
	arrows: true,
	dots: false,
	centerMode: true,
	centerPadding: '20%',
	slidesToShow: 1,
	autoplay: true,
	autoplaySpeed: 2000,
	responsive: [
		{
      		breakpoint: 1700,
      		settings: {
      			centerPadding: '15%',
      		}
	    },
	    {
      		breakpoint: 992,
      		settings: {
      			arrows: false,
      		}
	    },
	  ]
})
$('.business_slider').on('afterChange',function(event, slick, direction){
	addSlickNumber(slick)
})
function addSlickNumber(slick){
	
	var total = slick.$slides.length;
	var current = slick.currentSlide + 1;
	
	$('#slick-current1').text(current)
	$('#slick-total1').text(total)
}

function getBoardInfo(seq,event,link){
	$(event.target).closest('.nav-pills').find('.nav-link').removeClass('active')
	$(event.target).addClass('active')
		$.ajax({
			url: '/mainGallery'
			,type: 'post'
			,data: { bcseq: seq }
			,async: true
			,success: function(response) {
				try {
					var list = response.data;
					for (i = 0; i < 5; i++) {
						console.log(list[i])
						var subject = list[i].subject != ''?list[i].subject:'';
						var body = list[i].body != ''?list[i].body:'';
						var src= "${UPLOAD_ROOT}"+list[i].fileVOList[0].saveFilename;
						$('#list-item'+i+' img').attr('src',src);
						$('#list-item'+i+' h5').text(subject);
						$('#list-item'+i+' p').text(body);
						$('#list-item'+i+' .link').attr('href',link+list[i].boardSeq);
					}
				}
				catch(e) {
				
				}
			}
		}
		);
}	
function showVideo(){
	$('#home_video').removeClass('d-none')	
	
}
</script>
